                //console.log("Upload Completed!");
                //$('.qq-upload-success').remove();
                Notify(NotificationType.Success, "Logo uploaded successfully.");
            }
        }
    });
    $('#trigger-upload').click(function () {
        uploader.uploadStoredFiles();
    });
