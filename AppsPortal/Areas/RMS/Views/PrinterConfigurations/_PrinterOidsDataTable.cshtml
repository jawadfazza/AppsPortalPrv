@model MasterRecordStatus
<h3 class="Title">Oids</h3>
<div class="row">
    <div class="col-lg-5 col-md-6 col-sm-8 col-xs-12">
        @if (Model.IsParentActive)
        {
            <div id=@string.Format("{0}Filter", DataTableNames.PrinterOidsDataTable) class="filter-section" style="display:none;">
                <div class="row">
                    <div class="col-lg-12">
                        @Html.Partial("_RecordStatus", DataTableNames.PrinterOidsDataTable)
                    </div>
                </div>
            </div>
            <div class="dt-controls-field" id="@string.Format("{0}ActionsContainer", DataTableNames.PrinterOidsDataTable)">
                @Html.FieldDataTableFilter(DataTableNames.PrinterOidsDataTable)
                @Html.FieldDataTableCreate(Permissions.PrintersConfiguration.Create, Apps.RMS, Url.Action("PrinterOIDCreate", "PrinterConfiguration", new { FK = Model.ParentGUID }))
                @Html.FieldDataTableDelete(Permissions.PrintersConfiguration.Delete, Apps.RMS, DataTableNames.PrinterOidsDataTable)
                @Html.FieldDataTableRestore(Permissions.PrintersConfiguration.Restore, Apps.RMS, DataTableNames.PrinterOidsDataTable)
                @Html.FieldDataTableRefresh(DataTableNames.PrinterOidsDataTable)
            </div>
        }
        @Html.Partial("DataTableControl", new DataTableControl { Name = DataTableNames.PrinterOidsDataTable, FormController = "PrinterConfiguration", Area = "RMS", Type = DataTableTypes.Field, EditMode = DataTableEditMode.Modal, PK = Model.ParentGUID, OrderBy = 1 })
    </div>
</div>