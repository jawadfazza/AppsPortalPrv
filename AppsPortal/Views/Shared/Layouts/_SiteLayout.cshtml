        if (window.localStorage.getItem('fullScreen') == 'true') {
            document.write('<link href="/Assets/Style/FullScreen.css" rel="stylesheet" />');
        }
    </script>
    <script src="~/Assets/Plugin/fullcalendar/fullcalendar.js"></script>
    <link href="~/Assets/Plugin/fullcalendar/fullcalendar.css" rel="stylesheet" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
</head>
<body class="@(Languages.IsRighToLeft() ? "rtl" : "ltr")">
    <div class="mbl--head">
        <div class="logo">
            <a><img src="~/Assets/images/unhcr-logo.png" alt="UNHCR logo" /></a>
        </div>
        <div class="mbl--burger">
            <button class="c-hamburger c-hamburger--htx">
                <span>toggle menu</span>
            </button>
        </div>
    </div>
    <header class="">
        @Html.Partial("_SiteHeader")
        @Html.Partial("_SiteNavigation")
    </header>
    <div class="body--wrapper">
        <div class="overlay"></div>
        <div class="main-content" dir="@(Languages.IsRighToLeft() ? "rtl" : "ltr")">
            <input type="hidden" id="TabGUID" />
            @Html.Partial("_SiteSideMenu")
            @RenderBody()
            @Html.Partial("_Modal")
            @Html.Partial("_Confirm")
            @Html.Partial("_SignalRModal")
        </div>
        <i id="scroller" class="fa fa-angle-up fa-4x scrollToTop" onclick="goTop();"></i>

        <div id="PopOverContainer">
        </div>
        @Html.Partial("_SiteFooter")
    </div>
    <script src="~/Assets/Plugin/JQuery/jquery.counterup.min.js"></script>
    <script src="~/Assets/Plugin/JQuery/jquery.validate.min.js"></script>
