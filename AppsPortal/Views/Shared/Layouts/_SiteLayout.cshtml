    <script src="~/Assets/Plugin/Mojs/mo.min.js"></script>
    <script src="~/Assets/Plugin/Noty/noty.js"></script>
    <script src="~/Assets/JScript/JScript.js"></script>


    @RenderSection("scripts", false)

