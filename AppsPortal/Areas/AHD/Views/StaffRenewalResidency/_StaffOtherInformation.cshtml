@model StaffRenwalResidencyModel
<div class=" checkboxs-form">
    <div class="row">
        
        

        <div class="form-group">
            @Html.Label("UNLP Expiry Date")
            <br />
            @Html.DisplayFor(model => model.PassportValidityDate, new { htmlAttributes = new { @class = "form-control PageTitle", autocomplete = "off" } })

        </div>

        <div class="form-group">
            @Html.Label("Expiry Visa Date")
            <br />
            @Html.DisplayFor(model => model.CurrentResidencyEndDateSent, new { htmlAttributes = new { @class = "form-control PageTitle", autocomplete = "off" } })

        </div>

    </div>
</div>
