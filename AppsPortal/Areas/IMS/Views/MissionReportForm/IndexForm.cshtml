    Layout = "~/Views/Shared/Layouts/_SiteLayout.cshtml";
}
@Html.PageHeader("31d355fc-156a-4a68-b718-68d5ca2b4309")


<div class="page--content">
    <div id=@string.Format("{0}Filter", DataTableNames.MissionReportFormTrackingDataTable) class="filter-section" style="display:none;" DefaultButton="btnFind">
        <div class="row">
            <div class="col-lg-12">
                @Html.Partial("_FilterField", new FilterField { LabelText = resxDbFields.GovernorateGUID, DbColumnName = "GovernorateGUID", FilterForDataType = FilterDataTypes.Options, DataList = new DropDownList().Governorates(Guid.Parse("699287E8-754D-4A63-B8DD-5344CFBAFD22")), IsMultiple = true, IsSearchable = true })
                @Html.Partial("_FilterField", new FilterField { LabelText = resxDbFields.DistrictGUID, DbColumnName = "DistrictGUID", FilterForDataType = FilterDataTypes.Options, DataList = new DropDownList().AllDistricts(), IsSearchable = true })
            </div>
            <div class="col-lg-12">
                @Html.Partial("_FilterField", new FilterField { LabelText = resxDbFields.SubDistrictGUID, DbColumnName = "SubDistrictGUID", FilterForDataType = FilterDataTypes.Options, DataList = new DropDownList().AllSubDistricts(), IsSearchable = true })
                @Html.Partial("_FilterField", new FilterField { LabelText = resxDbFields.CommunityGUID, DbColumnName = "CommunityGUID", FilterForDataType = "Text", DataList = new DropDownList().AllCommunities(), IsMultiple = true })
