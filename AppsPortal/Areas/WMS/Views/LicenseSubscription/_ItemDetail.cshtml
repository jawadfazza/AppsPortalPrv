@model WMSContractSTIItemUpdateModel
<div class="col-md-12">
    <div class="col-md-12">
        @Html.AntiForgeryToken()
        @Html.HiddenFor(model => model.ItemInputDetailGUID)


        <div class="form-group">
            @Html.LabelFor(model => model.ModelName)
            @Html.DisplayFor(model => model.ModelName, new { htmlAttributes = new { @class = "form-control", autocomplete = "off", @Value = 1 } })
        </div>
        <div class="form-group">
            @Html.LabelFor(model => model.BarcodeNumber)
            @Html.DisplayFor(model => model.BarcodeNumber, new { htmlAttributes = new { @class = "form-control", autocomplete = "off", @Value = 1 } })
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.SerialNumber)
            @Html.DisplayFor(model => model.SerialNumber, new { htmlAttributes = new { @class = "form-control", autocomplete = "off", @Value = 1 } })
        </div>


    </div>
</div>

