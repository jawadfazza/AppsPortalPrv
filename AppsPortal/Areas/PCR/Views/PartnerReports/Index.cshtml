@{
    ViewBag.Title = "JobTitles";
    Layout = "~/Views/Shared/Layouts/_SiteLayout.cshtml";
}
@Html.PageHeader("19724930-f6bd-48c5-8081-1938e720b224")
<div class="page--content">
    <div id=@string.Format("{0}Filter", DataTableNames.PartnerReportsDataTable) class="filter-section" style="display:none;" DefaultButton="btnFind">
        <div class="row">
            <div class="col-lg-12">
                @Html.Partial("_FilterField", new FilterField { LabelText = resxDbFields.CommunityCenters, DbColumnName = "PartnerCenterDescription", FilterForDataType = "Text" })
                @Html.Partial("_FilterField", new FilterField { LabelText = resxDbFields.Governorate, DbColumnName = "DutyStationGUID", FilterForDataType = "Options", DataList = new DropDownList().DutyStationsByProfile(), IsMultiple = true })
            </div>
            <div class="col-lg-12">
                @Html.Partial("_FilterField", new FilterField { LabelText = resxDbFields.Partner, DbColumnName = "OrganizationInstanceGUID", FilterForDataType = "Options", DataList = new DropDownList().OrganizationsInstancesAcronymByProfile(), IsMultiple = true })
                @Html.Partial("_FilterField", new FilterField { LabelText = resxDbFields.Active, DbColumnName = "Active", FilterForDataType = "Boolean", MustHasInitValue = true, DataList = new DropDownList().RecordStatus() })
            </div>
        </div>


        <div class="form-group filter-actions">
            @Html.DatatableFilterControls(DataTableNames.PartnerReportsDataTable)
        </div>
    </div>
    <div class="dt-controls-index" id="@string.Format(" {0}ActionsContainer", DataTableNames.PartnerReportsDataTable)">
        @Html.IndexDatatableFilter(DataTableNames.PartnerReportsDataTable)
        @Html.IndexDatatableCreate(Permissions.UploadPartnerReport.Create, Apps.PCR, "/PCR/PartnerReports/Create")
        @Html.IndexDatatableDelete(Permissions.UploadPartnerReport.Delete, Apps.PCR, DataTableNames.PartnerReportsDataTable)
        @Html.IndexDatatableRestor(Permissions.UploadPartnerReport.Restore, Apps.PCR, DataTableNames.PartnerReportsDataTable)
        @Html.IndexDatatableRefresh(DataTableNames.PartnerReportsDataTable)
        @*@Html.FieldDataTableUpload(Permissions.UploadPartnerReport.Create, Apps.PCR, Url.Action("BulkPartnerReportCompiledCreate", "PartnerReports"))*@
    </div>
    <div class="page--body">
        @Html.Partial("DataTableControl", new DataTableControl { Name = DataTableNames.PartnerReportsDataTable, Area = "PCR", Type = DataTableTypes.Index, EditMode = DataTableEditMode.Page, OrderBy = 1 })
    </div>
</div>
