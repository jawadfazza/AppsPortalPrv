        @Html.LabelFor(model => model.BrowserName)
        @Html.EditorFor(model => model.BrowserName, new { htmlAttributes = new { @class = "form-control ays-ignore" } })
    </div>
    <div class="form-group">
        @Html.LabelFor(model => model.UserHostAddress)
        @Html.EditorFor(model => model.UserHostAddress, new { htmlAttributes = new { @class = "form-control ays-ignore" } })
