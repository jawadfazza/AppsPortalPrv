    <div class="panel panel-default">

        <div class="panel-heading" role="tab" id="CountriesHeader" data-url="@Url.Action("GetPPAFilesHistory","Reports",new {Area = "PPA", FK =  item.PPAOriginalFileGUID})" data-toggle="collapse" data-parent="#accordionLVL2" href="#@item.PPAOriginalFileGUID" aria-expanded="false">
