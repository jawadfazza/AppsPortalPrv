<script>
    $('#staffProfileConfirmationLoader').hide();
    var ConfirmProfileStaffInofrmation = function () {


        var userGUID = "@Model.UserGUID";


        var url = "/ORG/StaffProfile/ConfirmProfileStaffInofrmation/";

        $("#staffProfileConfirmationLoader").show();
        $(':button').prop('disabled', true);
        $.ajax({
            url: url,
            method: "POST",
            data: {
                userGUID: userGUID,


            },
            success: function (data) {
                $("#staffProfileConfirmationLoader").hide();

                $(':button').prop('disabled', false);
                if (data.success == 1) {


                    Notify(NotificationType.Success, 'Saved Successfully');
                    $("#FeedbackControlersFormControls").hide();

                }
                else if (data.success == 0) {

                    return true;
                }
                else if (data.success == -2) {
                    Notify(NotificationType.Warning, 'No Permission');
                }
            }
        });
    }


    




</script>