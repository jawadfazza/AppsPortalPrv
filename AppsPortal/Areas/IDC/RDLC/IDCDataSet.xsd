                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        IDC.dataCardIndividualInfo.IndividualID, IDC.dataCardIndividualInfo.CaseNumber, IDC.dataCardIndividualInfo.FullName_EN, IDC.dataCardIndividualInfo.FullName_AR, IDC.dataCardIndividualInfo.Sex, 
                         IDC.dataCardIndividualInfo.DateOfBrith, IDC.dataCardIndividualInfo.Category, IDC.dataCardIndividualInfo.ArrivalDate, IDC.dataCardIssued.IssueDate, IDC.dataCardIssued.ExpirtionDate, IDC.dataCardIssued.Valid, 
                         IDC.dataCardIssued.Barcode, IDC.dataCardIssued.Photo, code.codeCountriesLanguages.Nationality AS Nationality_EN, 
                         CASE WHEN dataCardIndividualInfo.ShowNationality = 1 THEN codeCountriesLanguages_AR.Nationality ELSE codeCountriesLanguages_AR.CountryDescription END AS Nationality_AR, 
                         IDC.dataCardIndividualInfo.ArrivalEstimation
FROM            code.codeCountriesLanguages AS codeCountriesLanguages_AR INNER JOIN
                         code.codeCountries AS codeCountries_1 ON codeCountriesLanguages_AR.CountryGUID = codeCountries_1.CountryGUID AND codeCountriesLanguages_AR.CountryGUID = codeCountries_1.CountryGUID AND 
                         codeCountriesLanguages_AR.CountryGUID = codeCountries_1.CountryGUID INNER JOIN
                         code.codeCountries INNER JOIN
                         code.codeCountriesLanguages ON code.codeCountries.CountryGUID = code.codeCountriesLanguages.CountryGUID INNER JOIN
                         IDC.dataCardIndividualInfo ON code.codeCountries.CountryA3Code = IDC.dataCardIndividualInfo.CountyCodeA3 INNER JOIN
                         IDC.dataCardIssued ON IDC.dataCardIndividualInfo.CardIndividualInfoGUID = IDC.dataCardIssued.CardIndividualInfoGUID AND IDC.dataCardIndividualInfo.CardIndividualInfoGUID = IDC.dataCardIssued.CardIndividualInfoGUID ON
                          codeCountries_1.CountryA3Code = IDC.dataCardIndividualInfo.CountyCodeA3
WHERE        (IDC.dataCardIssued.CardIssuedGUID = @PK) AND (codeCountriesLanguages_AR.LanguageID = N'AR') AND (code.codeCountriesLanguages.LanguageID = N'EN')</CommandText>
