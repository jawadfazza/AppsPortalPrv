
@using (Html.BeginForm("SnmpAgent", "Home", new { Area = "RMS" }, FormMethod.Post, new { id = "PrinterConfigurationForm", @class = "main-form" }))
{
<h3>@ViewBag.result </h3>

    <div class="row">
        <div class="col-lg-7 col-md-5 col-sm-8 col-xs-12">
            <div class="form-group">
                @Html.Label("IP")
                @Html.Editor("IP", new { htmlAttributes = new { @class = "form-control" } })
            </div>
            <div class="form-group">
                @Html.Label("OID")
                @Html.Editor("OID", new { htmlAttributes = new { @class = "form-control" } })
            </div>
            <div>
                <input class="btn btn-primary " type="submit" value="Submit">
            </div>
        </div>
    </div>

}
