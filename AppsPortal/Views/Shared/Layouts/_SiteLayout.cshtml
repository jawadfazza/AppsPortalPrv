    <script src="~/Assets/Plugin/EasyTab/jquery.easytabs.min.js"></script>
    <script src="~/Assets/Plugin/matchHeight/jquery.matchHeight.min.js"></script>
    <script src="~/Assets/Plugin/FitText/jquery.fittext.min.js"></script>
    <script src="~/Assets/Plugin/UNHCR/app.min.js"></script>
    <script src="~/Assets/Plugin/JSZip/jszip.min.js"></script>
   
    <script src="~/Assets/Plugin/DataTable/Buttons/buttons.print.min.js"></script>
