            <div class="col-lg-12">
                @Html.Partial("_FilterField", new FilterField { LabelText = resxDbFields.MissionChallengesName, DbColumnName = "MissionChallenges", FilterForDataType = FilterDataTypes.Options, DataList = new DropDownList().codeMissionChallenges(), IsMultiple = true, IsSearchable = true })
                @Html.Partial("_FilterField", new FilterField { LabelText = resxDbFields.OngoingResponses, DbColumnName = "OngoingResponses", FilterForDataType = FilterDataTypes.Options, DataList = new DropDownList().codeOngoingResponses(), IsMultiple = true, IsSearchable = true })
