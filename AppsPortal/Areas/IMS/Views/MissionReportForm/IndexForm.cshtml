        @Html.Partial("DataTableControl", new DataTableControl { Name = DataTableNames.MissionReportFormTrackingDataTable, FormController = "MissionReportForm", Area = "IMS", Type = DataTableTypes.Index, EditMode = DataTableEditMode.Page, OrderBy = 1 })
